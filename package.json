{
  "name": "declare_it",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "precommit": "yarn flow && yarn test",
    "postinstall": "rn-nodeify --hack",
    "flow": "node_modules/.bin/flow",
    "format": "sort-package-json; import-sort --write '*.js'; prettier --write '*.js'; eslint --fix --ignore-pattern '!.eslintrc.js' '*.js';",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "node_modules/.bin/jest"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "@tradle/react-native-http": "^2.0.1",
    "browserify-zlib": "^0.1.4",
    "buffer": "^5.2.1",
    "console-browserify": "^1.1.0",
    "constants-browserify": "^1.0.0",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.2.0",
    "edge-core-js": "^0.11.1",
    "https-browserify": "0.0.1",
    "path-browserify": "0.0.0",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "16.5.0",
    "react-native": "0.57.1",
    "react-native-crypto": "^2.1.2",
    "react-native-fast-crypto": "^1.5.6",
    "react-native-fs": "^2.11.15",
    "react-native-level-fs": "^3.0.1",
    "react-native-os": "^1.2.1",
    "react-native-randombytes": "git://github.com/EdgeApp/react-native-randombytes.git",
    "react-native-tcp": "git://github.com/EdgeApp/react-native-tcp.git",
    "react-native-udp": "^2.3.1",
    "readable-stream": "^1.0.33",
    "string_decoder": "^0.10.31",
    "timers-browserify": "^1.4.2",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.6.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.81.0",
    "husky": "^1.0.0-rc.15",
    "import-sort-cli": "^5.2.0",
    "import-sort-parser-babylon": "^5.2.0",
    "import-sort-style-module": "^5.0.0",
    "jest": "23.6.0",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "^0.45.6",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "rn-nodeify": "^10.0.0",
    "sort-package-json": "^1.15.0"
  },
  "importSort": {
    ".js, .es": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "lint-staged": {
    "*.js": [
      "import-sort --write",
      "prettier --write",
      "eslint --fix --ignore-pattern '!.eslintrc.js'",
      "git add"
    ]
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  }
}
